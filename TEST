type integer;
type boolean = enum (true, false);
procedure proc1 (integer) -> integer;
procedure sub (integer, integer) -> integer;

process A = (
? integer a;
! integer b;
)
(
| submodule B(b)(a)
|)
where

	process B = (
	? integer c;
	! integer d;
	)
	(| d := c
	|)where

		process C = (
		? integer e;
		! integer f;
		)
		(| submodule A(f)(e)
		|)end;

	end;

end;
