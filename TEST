type integer;
type boolean = enum (true, false);
procedure proc1 (integer) -> integer;
procedure sub (integer, integer) -> integer;



process G1 = (
? integer z;
  integer x;
! integer y;
)
(
| y :=  ((2+x) * z) - (3 +x)
| submodule P3 (y)(z)
|)
where process P3 = (
  ? integer a;
  ! integer b;
  )
  (| b := a
  |)end;
  process P0 = (
  ? integer c;
  ! integer d;
  )
  (| d := c
  |)end;
end;

process G2 = (
? integer l;
  integer m;
! integer n;
)
(
| n :=  ((2+m) * l) - (3 +m)
| n := m
|)
end;
